<% include partials/header %>
<div class="container">
    <h1><%= title %></h1>
    <div class="table-responsive">
        <table class="table table-striped table-hover">
            <tr>
                <th>Id</th>
                <th>Title</th>
                <th>Body</th>
            </tr>
            <% posts.forEach(function(post){ %>
            <tr>
                <td><%= post.id %> </td>
                <td><%= post.title %> </td>
                <td><%= post.body %> </td>
            </tr>
            <% }) %>

        </table>
    </div>
</div>
<% include partials/footer %>