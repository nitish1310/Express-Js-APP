<% include partials/header %>
<div class="container">
    <h1><%= title %></h1>
    <div class="table-responsive">
        <table class="table table-striped table-hover">
            <tr>
                <th>Id</th>
                <th>Title</th>

            </tr>
            <% albums.forEach(function(album){ %>
            <tr>
                <td><%= album.id %> </td>
                <td><%= album.title %> </td>
            </tr>
            <% }) %>

        </table>
    </div>
</div>
<% include partials/footer %>